\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\left\{ X_{n}\right\} _{n=1}^{N}\overset{iid}{\sim}N_{4}\left(0,\Sigma\right)$
\end_inset

 where
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Sigma=\left(\begin{array}{cccc}
\sigma_{1}^{2} & \rho_{12}\sigma_{1}\sigma_{2} & \rho_{13}\sigma_{1}\sigma_{3} & \rho_{14}\sigma_{1}\sigma_{4}\\
 & \sigma_{2}^{2} & \rho_{23}\sigma_{2}\sigma_{3} & \rho_{24}\sigma_{2}\sigma_{4}\\
\vdots &  & \sigma_{3}^{2} & \rho_{34}\sigma_{3}\sigma_{4}\\
 & \cdots &  & \sigma_{4}^{2}
\end{array}\right)\iff\Sigma_{ij}=\rho_{ij}\cdot\sigma_{i}\cdot\sigma_{j}
\]

\end_inset


\end_layout

\begin_layout Standard
Now denote
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\xi_{N}=N^{-1}\left(\begin{array}{c}
\stackrel[n=1]{N}{\sum}X_{n1}X_{n1}\\
\stackrel[n=1]{N}{\sum}X_{n1}X_{n2}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n1}X_{n4}\\
\stackrel[n=1]{N}{\sum}X_{n2}X_{n2}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n2}X_{n4}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n4}X_{n4}
\end{array}\right)
\]

\end_inset


\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Subsection
Finding 
\begin_inset Formula $E\left[\xi_{N}\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E\left[\stackrel[n=1]{N}{\sum}X_{ni}X_{nj}\right] & =\stackrel[n=1]{N}{\sum}E\left[X_{ni}X_{nj}\right]\\
 & =\stackrel[n=1]{N}{\sum}\left(E\left[X_{ni}X_{nj}\right]-E\left[X_{ni}\right]E\left[X_{nj}\right]\right)\\
 & =\stackrel[n=1]{N}{\sum}Cov\left(X_{ni},X_{nj}\right)\\
 & =\stackrel[n=1]{N}{\sum}\rho_{ij}\cdot\sigma_{i}\cdot\sigma_{j}\\
 & =N\cdot\rho_{ij}\cdot\sigma_{i}\cdot\sigma_{j}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\left[\xi_{n}\right]=\left(\begin{array}{c}
\sigma_{1}^{2}\\
\rho_{12}\sigma_{1}\sigma_{2}\\
\vdots\\
\rho_{14}\sigma_{1}\sigma_{4}\\
\sigma_{2}^{2}\\
\vdots\\
\rho_{24}\sigma_{2}\sigma_{4}\\
\vdots\\
\sigma_{4}^{2}
\end{array}\right)
\]

\end_inset


\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Subsection
Finding 
\begin_inset Formula $Var\left(\xi_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Cov\left(\stackrel[n=1]{N}{\sum}X_{ni}X_{nj},\stackrel[n=1]{N}{\sum}X_{nk}X_{nl}\right) & =\stackrel[n=1]{N}{\sum}\stackrel[m=1]{N}{\sum}Cov\left(X_{ni}X_{nj},X_{mk}X_{ml}\right)\\
 & =\stackrel[n=1]{N}{\sum}Cov\left(X_{ni}X_{nj},X_{nk}X_{nl}\right)\\
 & =\ldots=N\cdot\left(\Sigma_{ik}\Sigma_{jl}+\Sigma_{il}\Sigma_{jk}\right)\\
 & =N\cdot\left(\rho_{ik}\rho_{jl}\cdot\sigma_{i}\cdot\sigma_{j}\cdot\sigma_{k}\cdot\sigma_{l}+\rho_{jk}\rho_{il}\cdot\sigma_{i}\cdot\sigma_{j}\cdot\sigma_{k}\cdot\sigma_{l}\right)\\
 & =N\cdot\left(\rho_{ik}\rho_{jl}+\rho_{jk}\rho_{il}\right)\cdot\left(\sigma_{i}\cdot\sigma_{j}\cdot\sigma_{k}\cdot\sigma_{l}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\xi_{n}=N^{-1}\left(\begin{array}{c}
\stackrel[n=1]{N}{\sum}X_{n1}X_{n1}\\
\stackrel[n=1]{N}{\sum}X_{n1}X_{n2}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n1}X_{n4}\\
\stackrel[n=1]{N}{\sum}X_{n2}X_{n2}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n2}X_{n4}\\
\vdots\\
\stackrel[n=1]{N}{\sum}X_{n4}X_{n4}
\end{array}\right)\rightsquigarrow N_{10}\left(\left(\begin{array}{c}
\sigma_{1}^{2}\\
\rho_{12}\sigma_{1}\sigma_{2}\\
\vdots\\
\rho_{14}\sigma_{1}\sigma_{4}\\
\sigma_{2}^{2}\\
\vdots\\
\rho_{24}\sigma_{2}\sigma_{4}\\
\vdots\\
\sigma_{4}^{2}
\end{array}\right),\Sigma^{\star}\right)
\]

\end_inset


