\begin_layout Section
The Univariate Case
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\left\{ X_{i},Y_{i}\right\} _{i=1}^{n}\overset{iid}{\sim}N_{2}\left(0,\left(\begin{array}{cc}
\sigma_{x}^{2} & \rho\sigma_{x}\sigma_{y}\\
\rho\sigma_{x}\sigma_{y} & \sigma_{y}^{2}
\end{array}\right)\right)$
\end_inset

 And denote 
\begin_inset Formula 
\[
\xi_{n}=n^{-1}\left(\begin{array}{c}
\stackrel[i=1]{n}{\sum}X_{i}Y_{i}\\
\stackrel[i=1]{n}{\sum}X_{i}^{2}\\
\stackrel[i=1]{n}{\sum}Y_{i}^{2}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Finding 
\begin_inset Formula $E\left[\xi_{n}\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E\left[\stackrel[i=1]{n}{\sum}X_{i}Y_{i}\right] & =\stackrel[i=1]{n}{\sum}E\left[X_{i}Y_{i}\right]\\
 & =\stackrel[i=1]{n}{\sum}E\left[X_{i}Y_{i}\right]\\
 & =\stackrel[i=1]{n}{\sum}\left(E\left[X_{i}Y_{i}\right]-E\left[X_{i}\right]E\left[Y_{i}\right]\right)\\
 & =\stackrel[i=1]{n}{\sum}Cov\left(X_{i},Y_{i}\right)\\
 & =n\cdot\rho\sigma_{x}\sigma_{y}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E\left[\stackrel[i=1]{n}{\sum}X_{i}^{2}\right] & =E\left[\stackrel[i=1]{n}{\sum}\sigma_{x}^{2}\left(\frac{X_{i}-0}{\sigma_{x}}\right)^{2}\right]\\
 & =\sigma_{x}^{2}E\left[\stackrel[i=1]{n}{\sum}\left(\frac{X_{i}-0}{\sigma_{x}}\right)^{2}\right]\\
 & =n\cdot\sigma_{x}^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\left[\xi_{n}\right]=\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Subsection
Finding 
\begin_inset Formula $Var\left(\xi_{n}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Var\left(\stackrel[i=1]{n}{\sum}X_{i}Y_{i}\right) & =\stackrel[i=1]{n}{\sum}Var\left(X_{i}Y_{i}\right)\\
 & =\stackrel[i=1]{n}{\sum}E\left(\left(X_{i}Y_{i}\right)^{2}\right)-E^{2}\left(X_{i}Y_{i}\right)\\
 & =\stackrel[i=1]{n}{\sum}E\left(X_{i}X_{i}Y_{i}Y_{i}\right)-\left(\rho\sigma_{x}\sigma_{y}\right)^{2}\\
 & =\stackrel[i=1]{n}{\sum}\left(E\left[X_{i}X_{i}\right]E\left[Y_{i}Y_{i}\right]+E\left[X_{i}Y_{i}\right]E\left[X_{i}Y_{i}\right]+E\left[X_{i}Y_{i}\right]E\left[X_{i}Y_{i}\right]\right)-\left(\rho\sigma_{x}\sigma_{y}\right)^{2}\\
 & =\stackrel[i=1]{n}{\sum}\sigma_{x}^{2}\sigma_{y}^{2}+\left(\rho\sigma_{x}\sigma_{y}\right)^{2}+\left(\rho\sigma_{x}\sigma_{y}\right)^{2}-\left(\rho\sigma_{x}\sigma_{y}\right)^{2}\\
 & =\stackrel[i=1]{n}{\sum}\sigma_{x}^{2}\sigma_{y}^{2}+\rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}\\
 & =n\cdot\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Var\left(\stackrel[i=1]{n}{\sum}X_{i}^{2}\right) & =Var\left(\stackrel[i=1]{n}{\sum}\sigma_{x}^{2}\left(\frac{X_{i}-0}{\sigma_{x}}\right)^{2}\right)\\
 & =\sigma_{x}^{4}\cdot2n=2n\cdot\sigma_{x}^{4}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Diag\left(Var\left(\xi_{n}\right)\right)=n^{-1}\left(\begin{array}{ccc}
\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2}, & 2\sigma_{x}^{4}, & 2\sigma_{y}^{4}\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Cov\left(\stackrel[i=1]{n}{\sum}X_{i}Y_{i},\stackrel[i=1]{n}{\sum}X_{i}^{2}\right) & =\stackrel[i=1]{n}{\sum}\stackrel[j=1]{n}{\sum}Cov\left(X_{i}Y_{i},X_{j}^{2}\right)\\
 & =\stackrel[i=1]{n}{\sum}Cov\left(X_{i}Y_{i},X_{i}^{2}\right)\\
 & =\stackrel[i=1]{n}{\sum}E\left[X_{i}^{3}Y_{i}\right]-E\left[X_{i}Y_{i}\right]E\left[X_{i}^{2}\right]\\
 & =\stackrel[i=1]{n}{\sum}E\left[X_{i}X_{i}X_{i}Y_{i}\right]-\rho\sigma_{x}\sigma_{y}\cdot\sigma_{x}^{2}\\
 & =\stackrel[i=1]{n}{\sum}3\cdot E\left[X_{i}X_{i}\right]E\left[X_{i}Y_{i}\right]-\rho\sigma_{x}^{3}\sigma_{y}\\
 & =\stackrel[i=1]{n}{\sum}3\cdot\sigma_{x}^{2}\cdot\rho\sigma_{x}\sigma_{y}-\rho\sigma_{x}^{3}\sigma_{y}\\
 & =\stackrel[i=1]{n}{\sum}3\cdot\rho\sigma_{x}^{3}\sigma_{y}-\rho\sigma_{x}^{3}\sigma_{y}\\
 & =2n\cdot\rho\sigma_{x}^{3}\sigma_{y}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Cov\left(\stackrel[i=1]{n}{\sum}X_{i}^{2},\stackrel[i=1]{n}{\sum}Y_{i}^{2}\right) & =\stackrel[i=1]{n}{\sum}Cov\left(X_{i}^{2},Y_{i}^{2}\right)\\
 & =\stackrel[i=1]{n}{\sum}E\left[X_{i}X_{i}Y_{i}Y_{i}\right]-E\left[X_{i}^{2}\right]E\left[Y_{i}^{2}\right]\\
 & =\stackrel[i=1]{n}{\sum}E\left[X_{i}^{2}\right]E\left[Y_{i}^{2}\right]+2E\left[X_{i}Y_{i}\right]^{2}-\sigma_{x}^{2}\sigma_{y}^{2}\\
 & =\stackrel[i=1]{n}{\sum}\sigma_{x}^{2}\sigma_{y}^{2}+2\rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}-\sigma_{x}^{2}\sigma_{y}^{2}\\
 & =n\cdot2\rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\xi_{n}=n^{-1}\left(\begin{array}{c}
\stackrel[i=1]{n}{\sum}X_{i}Y_{i}\\
\stackrel[i=1]{n}{\sum}X_{i}^{2}\\
\stackrel[i=1]{n}{\sum}Y_{i}^{2}
\end{array}\right)\rightsquigarrow N_{3}\left(\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right),\frac{2}{n}\left(\begin{array}{ccc}
\frac{1}{2}\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2} & \rho\sigma_{x}^{3}\sigma_{y} & \rho\sigma_{x}\sigma_{y}^{3}\\
\rho\sigma_{x}^{3}\sigma_{y} & \sigma_{x}^{4} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}\\
\rho\sigma_{x}\sigma_{y}^{3} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2} & \sigma_{y}^{4}
\end{array}\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Subsection
Using the Delta Method
\end_layout

\begin_layout Standard
Now, denote 
\begin_inset Formula $g:\,\left\{ \left(x,y,z\right)\in\mathbb{R}^{3}:\:y,z>0\:x^{2}<y\cdot z\right\} \mapsto\left[-1,1\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
g\left(\begin{array}{c}
x\\
y\\
z
\end{array}\right)=\frac{x}{\sqrt{y\cdot z}}=x\cdot y^{-\nicefrac{1}{2}}\cdot z^{-\nicefrac{1}{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial g}{\partial x} & =y^{-\nicefrac{1}{2}}\cdot z^{-\nicefrac{1}{2}}\\
\frac{\partial g}{\partial y} & =x\cdot z^{-\nicefrac{1}{2}}\cdot\left(-\nicefrac{1}{2}\right)\cdot y^{-\nicefrac{3}{2}}\\
 & =\frac{-x}{2\sqrt{z\cdot y^{3}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\nabla g\left(\begin{array}{c}
x\\
y\\
z
\end{array}\right) & =\left(\begin{array}{c}
\frac{1}{\sqrt{y\cdot z}}\\
\frac{-x}{2\sqrt{y^{3}\cdot z}}\\
\frac{-x}{2\sqrt{y\cdot z^{3}}}
\end{array}\right)\\
g\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right) & =\frac{\rho\sigma_{x}\sigma_{y}}{\sqrt{\sigma_{x}^{2}\cdot\sigma_{y}^{2}}}=\rho\\
\nabla g\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right) & =\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset

Now
\begin_inset Formula 
\[
\nabla g^{t}\cdot Var\left(\Xi\right)\cdot\nabla g=\frac{2}{n}\cdot\left(\begin{array}{ccc}
\frac{1}{\sigma_{x}\cdot\sigma_{y}} & \frac{-\rho}{2\sigma_{x}^{2}} & \frac{-\rho}{2\sigma_{y}^{2}}\end{array}\right)\cdot\left(\begin{array}{ccc}
\frac{1}{2}\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2} & \rho\sigma_{x}^{3}\sigma_{y} & \rho\sigma_{x}\sigma_{y}^{3}\\
\rho\sigma_{x}^{3}\sigma_{y} & \sigma_{x}^{4} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}\\
\rho\sigma_{x}\sigma_{y}^{3} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2} & \sigma_{y}^{4}
\end{array}\right)\cdot\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{n}\cdot\left(\begin{array}{ccc}
\frac{\frac{1}{2}\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2}}{\sigma_{x}\cdot\sigma_{y}}-\frac{\rho\cdot\rho\sigma_{x}^{3}\sigma_{y}}{2\sigma_{x}^{2}}-\frac{\rho\cdot\rho\sigma_{x}\sigma_{y}^{3}}{2\sigma_{y}^{2}} & \frac{\rho\sigma_{x}^{3}\sigma_{y}}{\sigma_{x}\cdot\sigma_{y}}-\frac{\rho\sigma_{x}^{4}}{2\sigma_{x}^{2}}-\frac{\rho\cdot\rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}}{2\sigma_{y}^{2}} & \frac{\rho\sigma_{x}\sigma_{y}^{3}}{\sigma_{x}\cdot\sigma_{y}}-\frac{\rho\cdot\rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}}{2\sigma_{x}^{2}}-\frac{\rho\cdot\sigma_{y}^{4}}{2\sigma_{y}^{2}}\end{array}\right)\cdot\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\frac{1}{n}\cdot\left(\begin{array}{ccc}
\sigma_{x}\sigma_{y}+\rho^{2}\sigma_{x}\sigma_{y}-\rho^{2}\sigma_{x}\sigma_{y}-\rho^{2}\sigma_{x}\sigma_{y} & 2\rho\sigma_{x}^{2}-\rho\sigma_{x}^{2}-\rho^{3}\sigma_{x}^{2} & 2\rho\sigma_{y}^{2}-\rho\cdot\sigma_{y}^{2}-\rho^{3}\sigma_{y}^{2}\end{array}\right)\cdot\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\frac{1}{n}\cdot\left(\begin{array}{ccc}
\left(1-\rho^{2}\right)\sigma_{x}\sigma_{y} & \rho\sigma_{x}^{2}\left(1-\rho^{2}\right) & \rho\sigma_{y}^{2}\left(1-\rho^{2}\right)\end{array}\right)\cdot\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\left(1-\rho^{2}\right)}{n}\cdot\left(\begin{array}{ccc}
\sigma_{x}\sigma_{y} & \rho\sigma_{x}^{2} & \rho\sigma_{y}^{2}\end{array}\right)\cdot\left(\begin{array}{c}
\frac{1}{\sigma_{x}\cdot\sigma_{y}}\\
\frac{-\rho}{2\sigma_{x}^{2}}\\
\frac{-\rho}{2\sigma_{y}^{2}}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{\left(1-\rho^{2}\right)}{n}\cdot\left(\frac{\sigma_{x}\sigma_{y}}{\sigma_{x}\cdot\sigma_{y}}+\rho\sigma_{x}^{2}\cdot\frac{-\rho}{2\sigma_{x}^{2}}+\rho\sigma_{y}^{2}\cdot\frac{-\rho}{2\sigma_{y}^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{\left(1-\rho^{2}\right)^{2}}{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Subsection
Conclusion
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
g\left(\begin{array}{c}
x\\
y\\
z
\end{array}\right) & =\frac{x}{\sqrt{y\cdot z}}\\
g\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right) & =\frac{\rho\sigma_{x}\sigma_{y}}{\sqrt{\sigma_{x}^{2}\cdot\sigma_{y}^{2}}}=\rho
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\xi_{n}=n^{-1}\left(\begin{array}{c}
\stackrel[i=1]{n}{\sum}X_{i}Y_{i}\\
\stackrel[i=1]{n}{\sum}X_{i}^{2}\\
\stackrel[i=1]{n}{\sum}Y_{i}^{2}
\end{array}\right)\rightsquigarrow N_{3}\left(\left(\begin{array}{c}
\rho\sigma_{x}\sigma_{y}\\
\sigma_{x}^{2}\\
\sigma_{y}^{2}
\end{array}\right),\frac{2}{n}\left(\begin{array}{ccc}
\frac{1}{2}\left(1+\rho^{2}\right)\sigma_{x}^{2}\sigma_{y}^{2} & \rho\sigma_{x}^{3}\sigma_{y} & \rho\sigma_{x}\sigma_{y}^{3}\\
\rho\sigma_{x}^{3}\sigma_{y} & \sigma_{x}^{4} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2}\\
\rho\sigma_{x}\sigma_{y}^{3} & \rho^{2}\sigma_{x}^{2}\sigma_{y}^{2} & \sigma_{y}^{4}
\end{array}\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\hat{\rho}_{n}=g\left(\xi_{n}\right)=\frac{\xi_{n,1}}{\sqrt{\xi_{n,2}\xi_{n,3}}}=\frac{\stackrel[i=1]{n}{\sum}X_{i}Y_{i}}{\sqrt{\stackrel[i=1]{n}{\sum}X_{i}^{2}\stackrel[i=1]{n}{\sum}Y_{i}^{2}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sqrt{n}\left(\hat{\rho}_{n}-\rho\right)\rightsquigarrow N\left(0,\;n^{-1}\cdot\left(1-\rho^{2}\right)^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\widehat{Var}\left(\hat{\rho}_{n}\right)\bot Var\left(X_{i}\right),Var\left(Y_{i}\right)
\]

\end_inset


\end_layout

